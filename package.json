{
  "name": "emails",
  "version": "1.1.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "create-template": "aws ses create-template --cli-input-json file://templates/reset-email.json",
    "update-template": "aws ses update-template --cli-input-json file://templates/reset-email.json",
    "send": "serverless invoke local --function send --path ./mocks/send-reset-email.json",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev -P 6008"
  },
  "author": "Lucas Nascimento",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spacenow-api/emails"
  },
  "devDependencies": {
    "aws-sdk": "~2.451.0",
    "serverless-domain-manager": "^3.3.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.10.1"
  },
  "dependencies": {
    "babel-runtime": "~6.26.0",
    "moment": "~2.24.0",
    "source-map-support": "~0.4.18",
    "uuid": "~3.3.2"
  }
}
